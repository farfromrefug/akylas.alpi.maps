{
  "titanium": {
    "ignores": [
      "**/{.,AUTHOR,COPYING,INSTALL,NEWS,PORTING,CHANGELOG,CONTRIBUTION,ChangeLog,Makefile,README,readme,LICENSE,sample,example,demo,htdocs}*",
      "**/{obj,obj.target,test,tests,doc,docs,msvc,Xcode,bin,demo}{,/**/*}",
      "**/*.{o,hprof,orig,pyc,pyo,rbc,swp,sln,dsp,dsw,cmd,vcproj,vcxproj,vcxproj.filters,pdb,exp,obj,lib,md,sh,gypi,gyp,h,cpp,yml,log,tlog,d.ts,Makefile,mk,c,cc,rc,xcodeproj,xcconfig}",
      "**/node_modules/**/*.html",
      "**/node_modules/{lodash}/*"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.52",
    "@types/requirejs": "^2.1.28",
    "@types/tinycolor": "^1.1.27",
    "babel-cli": "^6.18.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0"
  },
  "babel": {
    "presets": [
      "stage-0"
    ],
    "plugins": [
      "transform-es2015-template-literals",
      "transform-es2015-literals",
      "transform-es2015-function-name",
      "transform-es2015-arrow-functions",
      "transform-es2015-block-scoped-functions",
      "transform-es2015-classes",
      "transform-es2015-object-super",
      "transform-es2015-shorthand-properties",
      "transform-es2015-computed-properties",
      "transform-es2015-for-of",
      "transform-es2015-sticky-regex",
      "transform-es2015-unicode-regex",
      "check-es2015-constants",
      "transform-es2015-spread",
      "transform-es2015-parameters",
      "transform-es2015-destructuring",
      "transform-es2015-block-scoping",
      [
        "transform-es2015-modules-commonjs",
        {
          "allowTopLevelThis": true,
          "strict": false
        }
      ],
      [
        "transform-strict-mode",
        {
          "strict": false
        }
      ]
    ],
    "ignore": [
      "*.min.js",
      "**/bin/*",
      "**/test/*"
    ]
  }
}
