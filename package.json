{
  "titanium": {
    "ignores": [
      "**/{.,AUTHOR,COPYING,INSTALL,NEWS,PORTING,CHANGELOG,CONTRIBUTION,ChangeLog,Makefile,README,readme,LICENSE,sample,example,demo,htdocs,composer.json,gulpfile}*",
      "**/{obj,obj.target,test,tests,doc,docs,msvc,Xcode,bin}{,/**/*}",
      "**/*.{sublime-workspace,o,hprof,orig,pyc,pyo,rbc,swp,sln,dsp,dsw,cmd,vcproj,vcxproj,vcxproj.filters,pdb,exp,obj,lib,md,sh,gypi,gyp,h,cpp,yml,log,tlog,d.ts,Makefile,mk,c,cc,rc,xcodeproj,xcconfig,lock,map}",
      "**/node_modules/**/*browser*",
      "**/node_modules/**/*.{html,sql}",
      "**/node_modules/lodash/!(package.json|lodash.js)*",
      "**/node_modules/moment/{min,src,package.js,ender.js}{,/**/*}",
      "**/node_modules/moment-timezone/{builds}{,/**/*}",
      "**/node_modules/moment-timezone/data/{meta,unpacked}{,/**/*}",
      "**/node_modules/moment/locale/!(fr.js)",
      "**/node_modules/source-map/dist{,/**/*}",
      "**/node_modules/kalmanjs/lib/kalman.min.js",
      "**/node_modules/suncalc/!(suncalc.js|package.json)",
      "**/node_modules/string-score/bench{,/**/*}",
      "**/node_modules/tinycolor2/dist{,/**/*}",
      "**/ffwdme{,/**/*}",
      "**/yaku.core.js"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.80",
    "@types/nedb": "1.8.5",
    "@types/node": "^8.0.47",
    "@types/requirejs": "^2.1.31",
    "@types/tinycolor": "^1.1.27",
    "awesome-typescript-loader": "^3.4.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.4",
    "webpack": "^3.10.0"
  },
  "babel": {
    "plugins": [
      [
        "transform-es2015-modules-commonjs",
        {
          "allowTopLevelThis": true,
          "strict": false
        }
      ],
      [
        "transform-strict-mode",
        {
          "strict": false
        }
      ]
    ],
    "ignore": [
      "*.min.js",
      "**/bin/*",
      "**/test/*"
    ]
  }
}
